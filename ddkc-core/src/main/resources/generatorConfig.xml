<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>


	<context id="baseset" targetRuntime="MyBatis3Simple"
		defaultModelType="flat">

		<!-- 根据Mapper生成实体类,xml文件 -->
		<plugin type="tk.mybatis.mapper.generator.MapperPlugin">
			<property name="mappers"
				value="com.yykj.system.commons.service.MyMapper" />
		</plugin>
		<!-- 连接字符串 -->
		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://127.0.0.1:3306/ddkc" userId="root"
			password="root">
		</jdbcConnection>

		<!-- 指定生成“entity实体类、mybatis映射xml文件、mapper接口”的具体位置 -->
		<javaModelGenerator
			targetPackage="com.yykj.ddkc.entity"
			targetProject="src/main/java">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>

		<sqlMapGenerator
			targetPackage="com.yykj.ddkc.dao"
			targetProject="src/main/java">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>

		<javaClientGenerator
			targetPackage="com.yykj.ddkc.dao"
			targetProject="src/main/java" type="XMLMAPPER">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>


