<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.yykj.system.dao.SystemSubManageMapper">

    <select id="selectAllTree" resultType="map">
        select
          tsm.id AS id,
          tsm.`value` AS resourceName,
          tsm.name AS resourceKey
        FROM
          t_sys_sub_manage tsm
    </select>
</mapper>